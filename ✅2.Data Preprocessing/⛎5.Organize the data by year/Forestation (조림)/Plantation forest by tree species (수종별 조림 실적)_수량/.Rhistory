results <- list()
for (file in files) {
# CSV íŒŒì¼ ì½ê¸°
data <- read.csv(file)
# ì—´ ì´ë¦„ í™•ì¸
col_names <- colnames(data)
# 3ë²ˆì§¸ ì—´ì˜ ê°’ ì¶”ì¶œ
