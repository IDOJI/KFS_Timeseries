}
# colnames ë‚´ ëª¨ë“  ë¬¸ìì—´ì„ í¬í•¨í•˜ëŠ” ì—´ ì°¾ê¸°
matching_cols <- col_names[start_col:end_col]
target_cols <- matching_cols[sapply(matching_cols, function(x) all(sapply(colnames, function(y) grepl(y, x))))]
if (length(target_cols) == 0) {
