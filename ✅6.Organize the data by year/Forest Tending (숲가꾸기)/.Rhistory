delete_non_year_files <- function(directory) {
# ëª¨ë“  csv íŒŒì¼ì„ ì¬ê·€ì ìœ¼ë¡œ ì°¾ê¸°
files <- list.files(directory, pattern = "\\.csv$", recursive = TRUE, full.names = TRUE)
