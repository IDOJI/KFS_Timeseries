year_regex <- "^[0-9]{4}.*$"
# íŒŒì¼ ì‚­ì œ ì¹´ìš´í„°
deleted_files <- 0
for (file in files) {
# íŒŒì¼ ì´ë¦„ì—ì„œ í™•ì¥ì ì œê±°
